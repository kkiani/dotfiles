<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.kiarash.clear-temps</string>
	<key>ProgramArguments</key>
	<array>
		<string>/bin/zsh</string>
        <string>-c</string>
		<string>$(rm -rf $HOME/projects/temp; mkdir $HOME/projects/temp)</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
    <key>StandardOutPath</key>
    <string>/tmp/com.clear-temps.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/com.clear-temps.log</string>
</dict>
</plist>
