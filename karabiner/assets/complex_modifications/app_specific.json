{
  "title": "App-Specific Keybindings",
  "rules": [
    {
      "description": "App-Specific - Mute Microphone in Slack",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["left_command", "left_shift"]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": ["left_command", "left_shift"]
            }
          ],
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": ["com.tinyspeck.slackmacgap"]
            }
          ]
        }
      ]
    },
